<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tmaslon.testapp">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>


    <permission
        android:name="com.example.tmaslon.testapp.ACCESS"
        android:protectionLevel="signature" >
    </permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".JenkinsClientApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".service.RefreshService"
            android:exported="false"
            android:permission="com.example.tmaslon.testapp.ACCESS">
        </service>


    </application>

</manifest>
